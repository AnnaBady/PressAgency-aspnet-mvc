@model IEnumerable<WebApplication5.Models.Question>

@{
    ViewBag.Title = "RecivedQuestions";
    Layout = "~/Views/Admin/_AdminNav.cshtml";
}

<h2>RecivedQuestions</h2>

@Html.Partial("_navbareditor")
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.post_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.username_Viewer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Question1)
        </th>


        <th>
            @Html.DisplayNameFor(model => model.Reply)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.post_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.username_Viewer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question1)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Reply)
            </td>

            <td>
                @Html.ActionLink("Reply", "Reply", new { id = item.QuestionNumber })

            </td>
        </tr>
    }

</table>
